{
	"name": "svelteui",
	"version": "0.3.1",
	"description": "SvelteUI Components Monorepo.",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"test": "vitest run",
		"test:watch": "vitest",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"c8": "^7.11.0",
		"micromatch": "^4.0.4",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"turbo": "latest",
		"vitest": "^0.5.5",
		"vitest-svelte-kit": "^0.0.6"
	},
	"keywords": [
		"Svelte",
		"Mantine",
		"UI",
		"Component Library"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Brisklemonade/svelteui.git"
	},
	"author": "Kamell Perry",
	"license": "MIT",
	"turbo": {
		"baseBranch": "origin/main",
		"npmClient": "npm",
		"pipeline": {
			"build": {
				"dependsOn": [
					"^build"
				],
				"outputs": [
					"dist/**",
					".build/**"
				]
			},
			"lint": {
				"outputs": []
			},
			"dev": {
				"cache": false
			}
		}
	},
	"type": "module"
}
